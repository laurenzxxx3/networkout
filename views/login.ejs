	<body class="homepage">
		<div id="big-header">			
				<h1>Login</h1>
		</div>
		<div id="form-wrapper">
			<form action="/login" method="post">
				<fieldset>					
					<p>
					  <label>Username</label>
					  <input type = "text"
							 id = "myText"
							 value = "Username"
							 name = "usrname" />
					</p>
					<p>
					  <label>Password</label>
					  <input type = "password"
							  id = "myPwd"
							  value = "password"
							  name = "password" />
					</p>
					<a href="/" class="button">Back</a>						
					<button type="submit" class="button alt">Login</button>								
				</fieldset>
			</form>
    	</div>
	</body>
</html>